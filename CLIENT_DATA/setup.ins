[Actions]
requiredWinstVersion >= "4.11.3.6"
SetLogLevel = 7

Message "Deaktiviere Audio Dienste Conexant ..."
DosInAnIcon_deactivate

[DosInAnIcon_deactivate]
;%SCRIPTPATH%\certmgr.exe -add -c "%SCRIPTPATH%\jugendschutzfilter.pem" -s -r localMachine root
sc config CxUtilSvc start= disabled
sc config CxUIUSvc start= disabled
sc config CxAudioSvc start= disabled
sc stop CxUtilSvc
sc stop CxUIUSvc
sc stop CxAudioSvc
